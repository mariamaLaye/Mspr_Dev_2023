
pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/mariamaLaye/Mspr_Dev_2023.git'
            }
        }
        
        stage('Compile') {
            steps {
                script {
                    // Activate the virtual environment
                    sh 'source venv/bin/activate'
                    
                    // Run any compile commands you may have
                    sh 'python setup.py compile'
                }
            }
        }
    }
}

