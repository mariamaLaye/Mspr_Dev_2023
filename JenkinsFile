pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                bat 'echo "Compiling..."'
                bat 'rmdir /s /q Mspr_Dev_2023'
                bat 'git clone https://github.com/mariamaLaye/Mspr_Dev_2023.git'
            }
        }

        stage('Checkout') {
            steps {
                git 'https://github.com/your-repo.git'
                // Replace the URL with your own repository URL
            }
        }

        stage('Build') {
            steps {
                bat 'echo "Building..."'
                // Add your build commands here
            }
        }
    }
}
