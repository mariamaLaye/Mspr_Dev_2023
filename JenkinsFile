
pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/mariamaLaye/Mspr_Dev_2023.git'
            }
        }

        stage('Build') {
            steps {
                script {
                    // Create a virtual environment
                    sh 'python -m venv venv'
                    
                    // Activate the virtual environment test 
                    sh 'source venv/bin/activate'
                    
                    // Install project dependencies
                    //sh 'pip install -r requirements.txt'
                    
                    // Run any build commands you may have
                    dir('https://github.com/mariamaLaye/Mspr_Dev_2023/blob/5794493973e4df43cbbfe6b7d343c3112ced7dcc/modele')
                    sh 'python Customer.py build'
                }
            }
        }
        
        stage('Compile') {
            steps {
                script {
                    // Activate the virtual environment
                    sh 'source venv/bin/activate'
                    
                    // Run any compile commands you may have
                    sh 'python Customer.py compile'
                }
            }
        }
    }
}

